{
	"name": "haodt/rylai",
	"description": "PHP document export to JSON or import to specific API for distribution around the web",
	"type": "library",
	"license": "MIT",
	"keywords": ["document", "php", "doc to json"],
	"authors": [{
		"name": "Hao",
		"email": "haodt1990@gmail.com"
	}],
	"require": {
		"goaop/parser-reflection": "^1.1",
		"phpdocumentor/reflection-docblock": "^3.1",
		"symfony/finder": "^3.2",
		"symfony/options-resolver": "^3.2",
		"elasticsearch/elasticsearch": "~2.0",
		"twig/twig": "^2.0",
		"symfony/filesystem": "^3.2"
	},
	"autoload": {
		"psr-4": {
			"Rylai\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Rylai\\Tests\\": "tests",
			"Rylai\\Fixtures\\": "fixtures"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^6.1"
	}
}
